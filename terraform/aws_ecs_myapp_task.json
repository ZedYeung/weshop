[
        {
            "command": [
                "gunicorn",
                "Weshop.wsgi:application",
                "--bind",
                "0.0.0.0:8001"
            ],
            "environment": [
                {
                    "name": "SECRET_KEY",
                    "value": "${django_secret_key}"
                },
                {
                    "name": "SQL_ENGINE",
                    "value": "django.db.backends.postgresql_psycopg2"
                },
                {
                    "name": "SQL_DATABASE",
                    "value": "weshop"
                },
                {
                    "name": "SQL_USER",
                    "value": "postgres"
                },
                {
                    "name": "SQL_PASSWORD",
                    "value": "${db_password}"
                },
                {
                    "name": "SQL_HOST",
                    "value": "db"
                },
                {
                    "name": "SQL_PORT",
                    "value": "5432"
                },
                {
                    "name": "DATABASE",
                    "value": "postgres"
                },
                {
                    "name": "FRONTEND_URL",
                    "value": "frontend:80"
                },
                {
                    "name": "TOKEN",
                    "value": "${token}"
                },
                {
                    "name": "AWS_ALB",
                    "value": "${aws_alb}"
                },
                {
                    "name": "DOMAIN",
                    "value": "${domain}"
                }
            ],
            "links": [
              "db"
            ],
            "essential": true,
            "memory": 128,
            "image": "${backend_url}",
            "mountPoints": [
                {
                    "containerPath": "/usr/local/weshop-backend/media",
                    "sourceVolume": "Media_Volume"
                }
            ],
            "name": "backend",
            "portMappings": [
                {
                    "containerPort": 8001
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "${log_group_name}",
                    "awslogs-region": "${log_group_region}"
                }
            }
        },
        {
            "environment": [
                {
                    "name": "POSTGRES_USER",
                    "value": "postgres"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "postgres"
                },
                {
                    "name": "POSTGRES_DB",
                    "value": "weshop"
                }
            ],
            "essential": true,
            "memory": 256,
            "image": "postgres:11.2-alpine",
            "mountPoints": [
                {
                    "containerPath": "/var/lib/postgresql/data/",
                    "sourceVolume": "Postgres_Data"
                }
            ],
            "name": "db",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "${log_group_name}",
                    "awslogs-region": "${log_group_region}"
                }
            }
        },
        {
            "environment": [
                {
                    "name": "OUT_PORT",
                    "value": "80"
                },
                {
                    "name": "TOKEN",
                    "value": "${token}"
                }
            ],
            "links": [
              "backend"
            ],
            "essential": true,
            "memory": 64,
            "image": "${frontend_url}",
            "mountPoints": [
                {
                    "containerPath": "/usr/local/weshop-backend/media",
                    "sourceVolume": "Media_Volume"
                }
            ],
            "name": "frontend",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 8080
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "${log_group_name}",
                    "awslogs-region": "${log_group_region}"
                }
            }
        }
    ]